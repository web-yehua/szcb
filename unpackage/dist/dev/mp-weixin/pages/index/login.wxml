<view class="main-container"><image class="icon-image" src="/static/images/car-login.jpg" mode="aspectFit"></image><view class="item-block"><image class="image" src="../../static/images/phone.jpg" mode="aspectFit"></image><input class="input" type="number" value="{{phone}}" placeholder="请输入手机号码" placeholder-style="color:#FFFFFF" maxlength="11" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" bindinput="__e"/></view><view class="item-block"><image class="image" src="../../static/images/secure.jpg" mode="aspectFit"></image><input class="input" type="number" value="{{verify}}" placeholder="请输入验证码" placeholder-style="color:#FFFFFF" maxlength="6" data-event-opts="{{[['input',[['__set_model',['','verify','$event',[]]]]]]}}" bindinput="__e"/><send-verify class="login-verify" vue-id="1" phone="{{phone}}" bind:__l="__l"></send-verify></view><view data-event-opts="{{[['tap',[['handleLogin',['$event']]]]]}}" class="btn-login" bindtap="__e">登 录</view><navigator class="back" url="index" open-type="switchTab">返回首页</navigator></view>